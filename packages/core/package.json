{
  "name": "@yunseorim1116/npm-count-core",
  "version": "0.0.3",
  "description": "Core functions for npm-pack-count",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf **/dist && tsup"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "sourceMap": true,
    "splitting": false,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
